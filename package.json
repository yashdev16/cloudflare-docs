		"astro-live-code": "^0.0.4",
		"date-fns": "^4.1.0",
		"detype": "1.0.12",
		"dompurify": "3.2.1",
		"dot-prop": "^9.0.0",
		"fast-glob": "^3.3.2",
		"github-slugger": "^2.0.0",
		"hastscript": "^9.0.0",
		"he": "^1.2.0",
		"instantsearch.css": "^8.5.1",
		"instantsearch.js": "^4.75.4",
		"jsonc-parser": "^3.3.1",
		"lz-string": "^1.5.0",
